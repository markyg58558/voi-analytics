        "sqlQuery": "MERGE `victims-of-ink-data-platform.voi_warehouse.client_notes` T\nUSING `victims-of-ink-data-platform.voi_warehouse.client_notes_stage` S\nON T.NOTE_ID = S.NOTE_ID\n\nWHEN MATCHED THEN\n  UPDATE SET\n    CLIENT_ID = S.CLIENT_ID,\n    CREATED_BY_TEAM_MEMBER_ID = S.CREATED_BY_TEAM_MEMBER_ID,\n    LAST_EDITED_BY_TEAM_MEMBER_ID = S.LAST_EDITED_BY_TEAM_MEMBER_ID,\n    BODY = S.BODY,\n    CREATED_AT = S.CREATED_AT,\n    LAST_EDITED_AT = S.LAST_EDITED_AT,\n    _loaded_at = CURRENT_TIMESTAMP()\n\nWHEN NOT MATCHED THEN\n  INSERT (\n    NOTE_ID,\n    CLIENT_ID,\n    CREATED_BY_TEAM_MEMBER_ID,\n    LAST_EDITED_BY_TEAM_MEMBER_ID,\n    BODY,\n    CREATED_AT,\n    LAST_EDITED_AT,\n    _loaded_at\n  )\n  VALUES (\n    S.NOTE_ID,\n    S.CLIENT_ID,\n    S.CREATED_BY_TEAM_MEMBER_ID,\n    S.LAST_EDITED_BY_TEAM_MEMBER_ID,\n    S.BODY,\n    S.CREATED_AT,\n    S.LAST_EDITED_AT,\n    CURRENT_TIMESTAMP()\n  );\n",
