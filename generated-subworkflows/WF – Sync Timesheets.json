        "sqlQuery": "MERGE `victims-of-ink-data-platform.voi_warehouse.timesheets` T\nUSING `victims-of-ink-data-platform.voi_warehouse.timesheets_stage` S\nON T.TIMESHEET_ID = S.TIMESHEET_ID\n\nWHEN MATCHED THEN\n  UPDATE SET\n    LOCATION_ID = S.LOCATION_ID,\n    TEAM_MEMBER_ID = S.TEAM_MEMBER_ID,\n    EXPECTED_START = S.EXPECTED_START,\n    ACTUAL_START = S.ACTUAL_START,\n    EXPECTED_END = S.EXPECTED_END,\n    ACTUAL_END = S.ACTUAL_END,\n    LOCATION = S.LOCATION,\n    TEAM_MEMBER = S.TEAM_MEMBER,\n    SOURCE = S.SOURCE,\n    CLOCK_IN = S.CLOCK_IN,\n    CLOCK_IN_TYPE = S.CLOCK_IN_TYPE,\n    CLOCK_IN_DEVIATION = S.CLOCK_IN_DEVIATION,\n    CLOCK_OUT = S.CLOCK_OUT,\n    CLOCK_OUT_TYPE = S.CLOCK_OUT_TYPE,\n    CLOCK_OUT_DEVIATION = S.CLOCK_OUT_DEVIATION,\n    PAID_BREAK_HOURS = S.PAID_BREAK_HOURS,\n    PAID_BREAK_MINUTES = S.PAID_BREAK_MINUTES,\n    UNPAID_BREAK_HOURS = S.UNPAID_BREAK_HOURS,\n    UNPAID_BREAK_MINUTES = S.UNPAID_BREAK_MINUTES,\n    HOURS_WORKED = S.HOURS_WORKED,\n    HOURS_SCHEDULED = S.HOURS_SCHEDULED,\n    HOURS_TOTAL_PAID = S.HOURS_TOTAL_PAID,\n    _loaded_at = CURRENT_TIMESTAMP()\n\nWHEN NOT MATCHED THEN\n  INSERT (\n    TIMESHEET_ID,\n    LOCATION_ID,\n    TEAM_MEMBER_ID,\n    EXPECTED_START,\n    ACTUAL_START,\n    EXPECTED_END,\n    ACTUAL_END,\n    LOCATION,\n    TEAM_MEMBER,\n    SOURCE,\n    CLOCK_IN,\n    CLOCK_IN_TYPE,\n    CLOCK_IN_DEVIATION,\n    CLOCK_OUT,\n    CLOCK_OUT_TYPE,\n    CLOCK_OUT_DEVIATION,\n    PAID_BREAK_HOURS,\n    PAID_BREAK_MINUTES,\n    UNPAID_BREAK_HOURS,\n    UNPAID_BREAK_MINUTES,\n    HOURS_WORKED,\n    HOURS_SCHEDULED,\n    HOURS_TOTAL_PAID,\n    _loaded_at\n  )\n  VALUES (\n    S.TIMESHEET_ID,\n    S.LOCATION_ID,\n    S.TEAM_MEMBER_ID,\n    S.EXPECTED_START,\n    S.ACTUAL_START,\n    S.EXPECTED_END,\n    S.ACTUAL_END,\n    S.LOCATION,\n    S.TEAM_MEMBER,\n    S.SOURCE,\n    S.CLOCK_IN,\n    S.CLOCK_IN_TYPE,\n    S.CLOCK_IN_DEVIATION,\n    S.CLOCK_OUT,\n    S.CLOCK_OUT_TYPE,\n    S.CLOCK_OUT_DEVIATION,\n    S.PAID_BREAK_HOURS,\n    S.PAID_BREAK_MINUTES,\n    S.UNPAID_BREAK_HOURS,\n    S.UNPAID_BREAK_MINUTES,\n    S.HOURS_WORKED,\n    S.HOURS_SCHEDULED,\n    S.HOURS_TOTAL_PAID,\n    CURRENT_TIMESTAMP()\n  );\n",
