        "sqlQuery": "MERGE `victims-of-ink-data-platform.voi_warehouse.cash_flow` T\nUSING `victims-of-ink-data-platform.voi_warehouse.cash_flow_stage` S\nON T.TRANSACTION_REF = S.TRANSACTION_REF\n\nWHEN MATCHED THEN\n  UPDATE SET\n    SALE_ID = S.SALE_ID,\n    LOCATION_ID = S.LOCATION_ID,\n    TEAM_MEMBER_ID = S.TEAM_MEMBER_ID,\n    PROCESSED_BY_TEAM_MEMBER_ID = S.PROCESSED_BY_TEAM_MEMBER_ID,\n    CLIENT_ID = S.CLIENT_ID,\n    PAYMENT_DATE = S.PAYMENT_DATE,\n    LOCATION = S.LOCATION,\n    TEAM_MEMBER = S.TEAM_MEMBER,\n    CLIENT = S.CLIENT,\n    PROCESSED_BY_TEAM_MEMBER = S.PROCESSED_BY_TEAM_MEMBER,\n    PAYER_DETAILS = S.PAYER_DETAILS,\n    RECIPIENT_DETAILS = S.RECIPIENT_DETAILS,\n    TRANSACTION_TYPE = S.TRANSACTION_TYPE,\n    APPOINTMENT_REF = S.APPOINTMENT_REF,\n    OPENING_BALANCE = S.OPENING_BALANCE,\n    AMOUNT = S.AMOUNT,\n    CLOSING_BALANCE = S.CLOSING_BALANCE,\n    CURRENCY = S.CURRENCY,\n    _loaded_at = CURRENT_TIMESTAMP()\n\nWHEN NOT MATCHED THEN\n  INSERT (\n    TRANSACTION_REF,\n    SALE_ID,\n    LOCATION_ID,\n    TEAM_MEMBER_ID,\n    PROCESSED_BY_TEAM_MEMBER_ID,\n    CLIENT_ID,\n    PAYMENT_DATE,\n    LOCATION,\n    TEAM_MEMBER,\n    CLIENT,\n    PROCESSED_BY_TEAM_MEMBER,\n    PAYER_DETAILS,\n    RECIPIENT_DETAILS,\n    TRANSACTION_TYPE,\n    APPOINTMENT_REF,\n    OPENING_BALANCE,\n    AMOUNT,\n    CLOSING_BALANCE,\n    CURRENCY,\n    _loaded_at\n  )\n  VALUES (\n    S.TRANSACTION_REF,\n    S.SALE_ID,\n    S.LOCATION_ID,\n    S.TEAM_MEMBER_ID,\n    S.PROCESSED_BY_TEAM_MEMBER_ID,\n    S.CLIENT_ID,\n    S.PAYMENT_DATE,\n    S.LOCATION,\n    S.TEAM_MEMBER,\n    S.CLIENT,\n    S.PROCESSED_BY_TEAM_MEMBER,\n    S.PAYER_DETAILS,\n    S.RECIPIENT_DETAILS,\n    S.TRANSACTION_TYPE,\n    S.APPOINTMENT_REF,\n    S.OPENING_BALANCE,\n    S.AMOUNT,\n    S.CLOSING_BALANCE,\n    S.CURRENCY,\n    CURRENT_TIMESTAMP()\n  );\n",
