        "sqlQuery": "MERGE `victims-of-ink-data-platform.voi_warehouse.deposits` T\nUSING `victims-of-ink-data-platform.voi_warehouse.deposits_stage` S\nON T.DEPOSIT_ID = S.DEPOSIT_ID\n\nWHEN MATCHED THEN\n  UPDATE SET\n    LOCATION_ID = S.LOCATION_ID,\n    TEAM_MEMBER_ID = S.TEAM_MEMBER_ID,\n    CLIENT_ID = S.CLIENT_ID,\n    APPOINTMENT_ID = S.APPOINTMENT_ID,\n    COLLECTION_DATE = S.COLLECTION_DATE,\n    REFUND_DATE = S.REFUND_DATE,\n    REDEMPTION_DATE = S.REDEMPTION_DATE,\n    APPOINTMENT_REF = S.APPOINTMENT_REF,\n    LOCATION = S.LOCATION,\n    TEAM_MEMBER = S.TEAM_MEMBER,\n    CLIENT = S.CLIENT,\n    STATUS = S.STATUS,\n    COLLECTIONS = S.COLLECTIONS,\n    REDEMPTIONS = S.REDEMPTIONS,\n    REFUNDS = S.REFUNDS,\n    CLOSING_BALANCE = S.CLOSING_BALANCE,\n    CURRENCY_CODE = S.CURRENCY_CODE,\n    _loaded_at = CURRENT_TIMESTAMP()\n\nWHEN NOT MATCHED THEN\n  INSERT (\n    DEPOSIT_ID,\n    LOCATION_ID,\n    TEAM_MEMBER_ID,\n    CLIENT_ID,\n    APPOINTMENT_ID,\n    COLLECTION_DATE,\n    REFUND_DATE,\n    REDEMPTION_DATE,\n    APPOINTMENT_REF,\n    LOCATION,\n    TEAM_MEMBER,\n    CLIENT,\n    STATUS,\n    COLLECTIONS,\n    REDEMPTIONS,\n    REFUNDS,\n    CLOSING_BALANCE,\n    CURRENCY_CODE,\n    _loaded_at\n  )\n  VALUES (\n    S.DEPOSIT_ID,\n    S.LOCATION_ID,\n    S.TEAM_MEMBER_ID,\n    S.CLIENT_ID,\n    S.APPOINTMENT_ID,\n    S.COLLECTION_DATE,\n    S.REFUND_DATE,\n    S.REDEMPTION_DATE,\n    S.APPOINTMENT_REF,\n    S.LOCATION,\n    S.TEAM_MEMBER,\n    S.CLIENT,\n    S.STATUS,\n    S.COLLECTIONS,\n    S.REDEMPTIONS,\n    S.REFUNDS,\n    S.CLOSING_BALANCE,\n    S.CURRENCY_CODE,\n    CURRENT_TIMESTAMP()\n  );\n",
        "options": {}
